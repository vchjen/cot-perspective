    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "serve": "^14.0.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.0",
    "unzipper": "^0.10.11"
  },
  "keywords": [],
  "description": "",
